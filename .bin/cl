#!/bin/bash
#colorscheme script
#inspired by u/dkeg

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
d=$'\e[1m'
t=$'\e[0m'
v=$'\e[7m'

cat << EOF

$f1███$d██$t $f2███$d██$t $f3███$d██$t $f4███$d██$t $f5███$d██$t $f6███$d██$t
$f1███$d██$t $f2███$d██$t $f3███$d██$t $f4███$d██$t $f5███$d██$t $f6███$d██$t
$f15███$d$f15██$1 $f15███$d$f15██$1 $f15███$d$f15██$1 $f15███$d$f15██$1 $f15███$d$f15██$1 $f15███$d$f15██$1
 red   grn   ylw   blu   mag   cya

EOF
